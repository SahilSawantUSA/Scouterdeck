{
  "name": "dashboard",
  "version": "1.0.0",
  "description": "",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build:osx": "electron-builder --mac",
    "build:win64": "electron-builder --win --x64"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SahilSawantUSA/Scouterdeck.git"
  },
  "author": "Sahil Sawant",
  "license": "MIT",
  "dependencies": {
    "mysql": "^2.18.1"
  },
  "build": {
    "productName": "scouterdeck",
    "appId": "org.comets.scouterdeck",
    "asar": true,
    "protocols": {
      "name": "Scouterdeck",
      "schemes": [
        "scouterdeck"
      ]
    },
    "win": {
      "target": [
        "zip",
        "nsis"
      ]
    },
    "mac": {
      "target": [
        "default"
      ],
      "icon": "./resources/icon.icns"
    },
    "nsis": {
      "shortcutName": "Scouterdeck",
      "artifactName": "Scouterdeck",
      "uninstallDisplayName": "Scouterdeck",
      "oneClick": false,
      "allowToChangeInstallationDirectory": true
    },
    "directories": {
      "buildResources": "./resources/installer/",
      "output": "./dist/",
      "app": "."
    }
  },
  "devDependencies": {
    "electron": "^23.1.3",
    "electron-builder": "^23.6.0"
  }
}
